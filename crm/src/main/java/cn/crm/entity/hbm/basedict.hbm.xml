<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.crm.entity.BaseDict" table="base_dict">
		<id name="dictId">
			<!-- 主键序列生成策略 -->
		</id>
		<property name="dictTypeCode" />
		<property name="dictTypeName" />
		<property name="dictItemName" />
		<property name="dictItemCode" />
		<property name="dictSort" />
		<property name="dictEnable" />
		<property name="dictMemo" />
	</class>

</hibernate-mapping>
